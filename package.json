{
  "name": "force-canvas",
  "description": "sanity check to get the canvas if input happens to be the context; errors if it can't return a canvas",
  "version": "0.0.0",
  "author": {
    "name": "Will Hoag",
    "email": "dev@willhoag.com",
    "url": "www.willhoag.com"
  },
  "bugs": {
    "url": "https://github.com/willhoag/force-canvas/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.0",
    "create-canvas": "^1.0.1",
    "phantomjs": "^1.9.17",
    "standard": "^4.5.3",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.1",
    "tape-run": "^1.0.0"
  },
  "homepage": "git://github.com/willhoag/force-canvas.git",
  "keywords": [
    "canvas",
    "sanity",
    "check",
    "convert",
    "html5"
  ],
  "license": "MIT",
  "main": "index.js",
  "browser": "browser.js",
  "maintainers": [
    {
      "name": "devhoag",
      "email": "dev@willhoag.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/willhoag/force-canvas.git"
  },
  "scripts": {
    "build:browser": "browserify -t babelify --standalone forceCanvas index.es6 --extension .es6 > browser.js",
    "build:node": "babel index.es6 > index.js",
    "build": "npm run build:node && npm run build:browser",
    "prepublish": "npm run test && npm run build",
    "test": "standard index.es6 && browserify -t babelify --extension .es6 test.es6 | tape-run | tap-spec"
  }
}
